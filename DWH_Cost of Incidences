SELECT o.country_code                                           AS country_code,
        o.store_address_id                                      AS store_address_id,
       o.id                                                     AS Order,
       o.partner_commission_percentage                          AS percentage_commission,
       REPLACE(ROUND(SUM(ori.charged_to_partner), 2), ',', '.') AS amount,
       CASE
           WHEN s.is_food = FALSE OR s.id IN (1) -- include the store ids https://docs.google.com/spreadsheets/u/1/d/1zzW9i2koerglEm2fknJWm2x9436W-yiZO7ZzSKqG7tA/edit#gid=1019216283
               THEN 'COSTE_INCIDENCIAS'
           ELSE 'COSTE_INC_MANUAL' END                          AS concept
FROM
    order_refund_incidents ori
    JOIN orders            o
             ON o.id = ori.order_id
    JOIN geography         g ON g.code = o.city_code
    JOIN store_addresses   sa ON sa.id = o.store_address_id
    JOIN stores            s ON s.id = sa.store_id
WHERE g.country_code IN ('ES')
  AND ori.update_time_local BETWEEN '2022-11-14 00:00:00'
    AND '2022-11-20 23:59:59' -- Put invoicing period. For example for 1Q sept '2020-09-01 00:00:00' AND '2020-09-15 23:59:59'
  AND o.creation_time_local BETWEEN '2022-10-01 00:00:00'
    AND '2022-11-20 23:59:59' -- Put month and a half taking the last day of the invoicing period as reference. For example for 1Q sept '2020-08-01 00:00:00' AND '2020-09-15 23:59:59'
  AND sa.company_detail_id IS NOT NULL
--  AND mcd_partner = FALSE     -- This condition is only applicable to SWE countries, GG countries should include mcdo
--  AND NOT (g.country_code IN ('IT', 'ES') AND s.store_name = 'Burger King')
AND o.store_address_id IN (178594,161117,164428,190067,190081,190049,165557,163952,164223,164232,165359,161694,161844,152145,152146,165314,161705,161667,161690,161704,156087,154889,156092,191815,165317,165287,161702,161718,156095,156106,165615,156117,156121,156128,156140,164924,165002,165009,165019,165025,164812,164840,164957,164968,164981,164987,164992,165014,165020,161109,163967,161586,161594,156177,164718,156180,158765,165558,156080,234785,165309,165312,161111,165280,165282,165286,165288,178252,165266,165244,165265,164499,165274,165247,165255,165260,164542,164573,164587,164595,158753,164603,164792,164810,164847,164900,165313,165291,182880,165425,165306,165621,165577,165578,173497,158735,158736,165332,164619,189785,158752,234769,165612,165330,164581,234792,161120,161123,165447,165328,150122,165618,152144,162033,164545,165262,185055,165264,165261,164553,165259,165263,164638,150106,164651,152163,165617,158708,165564,178264,163970,164756,163977,162423,161895,161134,161606,165316,156281,336752,158750,161739,173344,162271,162001,164652,164467,156711,161735,165620,152150,336686,176232,165327,161837,161892,161142,163998,164672,164492,164556,173454,156285,161621,161126,283869,161754,161077,158729,176221,161693,336703,161774,158743,156287,158747,158748,189767,156288,190083,165302,152176,173485,158761,158760,161848,161918,173502,164613,176177,535146,161963,313457,161118,178285,165574,164005,234738,178290,164826,394672,164011,165358,152165,150113,156297,165516,161936,176209,181029,234771,185172,178305,165579,161828,161778,191802,158763,152158,161711,161732,161741,173494,156315,161136,178307,158813,178062,161727,173506,178310,161127,176187,161681,173474,257008,183167,161841,156328,234781,161559,173492,156697,164715,161886,176215,161107,161939,173472,161133,178312,173348,456069,181032,156680,162429,180080,162404,165331,244271,170153,161764,206928,164684,165326,556462,173480,176191,425283,175114,161793,165271,156440,187196,234766,161977,165454,161775,161137,160929,161856,158762