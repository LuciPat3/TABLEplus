SELECT g.country_code,
       sa.id           AS store_address_id,
       sa.bank_account AS iban
FROM
    store_addresses sa
    JOIN stores     s ON s.id = sa.store_id
    JOIN geography  g ON g.code = s.city_code
WHERE g.country_code IN ('ES') /* add more countries in the () */
  AND sa.partner = TRUE
